{
	"manifest_version": 2,
	"name": "Autofill Job Applications",
	"version": "1.0",
	"permissions": ["storage"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "survey.png",
		"default_popup": "popup.html",
		"default_title": "a pop"
	}
}
